{
  "name": "sellekplaygroundfrontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.6.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "23.3.9",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.10",
    "@types/react": "^16.7.7",
    "@types/react-dom": "16.0.10",
    "@types/react-redux": "^6.0.10",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-scripts": "2.1.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.2",
    "typescript": "3.1.6"
  },
  "scripts": {
    "start": "serve -s build",
    "prestart": "npm install -g serve; npm run build;",
    "local": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "1.15.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0"
  },
  "lint-staged": {
    "{src,__{tests,mocks}__}/**/*.{js,ts,tsx}": [
      "prettier --write",
      "tslint",
      "git add"
    ],
    "{src,__{tests,mocks}__}/**/*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
